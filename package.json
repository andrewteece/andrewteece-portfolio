{
  "name": "andrewteece-portfolio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -p tsconfig.app.json && vite build && node scripts/prerender.mjs && tsx scripts/generate-rss.ts",
    "vercel-build": "tsc -p tsconfig.app.json && vite build && node scripts/prerender.mjs && tsx scripts/generate-rss.ts",
    "prerender": "node scripts/prerender.mjs",
    "typecheck": "tsc -p tsconfig.app.json --noEmit && tsc -p tsconfig.test.json --noEmit",
    "smoke": "npm run typecheck && npm run lint && npm run build",
    "lint": "eslint --cache --no-error-on-unmatched-pattern --ext .ts,.tsx src \"**/*.test.ts\" \"**/*.test.tsx\" \"**/*.spec.ts\" \"**/*.spec.tsx\"",
    "lint:fix": "npm run lint -- --fix",
    "lint:configs": "eslint --cache eslint.config.js postcss.config.js tailwind.config.js vite.config.*",
    "preview": "vite preview",
    "rss": "tsx scripts/generate-rss.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@dr.pogodin/react-helmet": "^3.0.2",
    "@mdx-js/mdx": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@mdx-js/rollup": "^3.1.0",
    "@tailwindcss/vite": "^4.1.12",
    "@vercel/speed-insights": "^1.2.0",
    "framer-motion": "^12.23.3",
    "lucide-react": "^0.525.0",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.6.3",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.1",
    "remark-mdx-frontmatter": "^5.2.0",
    "rss": "^1.2.2",
    "tailwindcss": "^4.1.12",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@storybook/addon-a11y": "^8.6.14",
    "@storybook/addon-docs": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-vitest": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/postcss": "^4.1.12",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/dom": "^10.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.30.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-storybook": "^0.8.0",
    "fast-glob": "^3.3.3",
    "globals": "^16.3.0",
    "gray-matter": "^4.0.3",
    "jsdom": "^26.1.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "storybook": "^8.6.14",
    "tailwind-scrollbar": "^4.0.2",
    "tsx": "^4.20.4",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vercel": "^46.0.1",
    "vite": "^7.1.1",
    "vitest": "^3.2.4"
  },
  "overrides": {
    "@testing-library/react": "^16.0.0"
  }
}
